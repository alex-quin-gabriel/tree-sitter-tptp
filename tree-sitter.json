{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "tptp",
      "camelcase": "TPTP",
      "title": "Thousand Problems for Theorem Provers",
      "scope": "source.tptp",
      "path": ".",
      "file-types": [
        "tff",
        "thf",
        "cnf",
        "fof",
        "p",
        "ax"
      ],
      "highlights": "queries/highlights.scm",
      "injection-regex": "^tptp$",
      "class-name": "TreeSitterTptp"
    }
  ],
  "metadata": {
    "version": "0.2.1",
    "license": "Apache 2.0",
    "description": "Tptp grammar for tree-sitter",
    "authors": [
      {
        "name": "Alex Quin Averie Gabriel",
        "email": "agabriel@4d6.de"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-tptp"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  },
  "theme": {
    "keyword": "purple",
    "function": "blue",
    "type": "green",
    "property": "green",
    "string": "green",
    "number": "brown"
  }
}
